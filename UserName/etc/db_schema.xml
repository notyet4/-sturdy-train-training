<?xml version="1.0"?>

<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="amasty_username_blacklist" resource="default" engine="innodb" comment="Blacklist Tab">
        <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="Blacklist Id" />
        <column xsi:type="varchar" name="sku" nullable="false" length="255" default="" comment="SKU" />
        <column xsi:type="int" name="qty" nullable="false" unsigned="true" comment="QTY" />
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id" />
        </constraint>
    </table>
</schema>